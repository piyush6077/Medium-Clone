#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/admin/3D Objects/A React 2025/Medium/Backend/node_modules/.pnpm/touch@3.1.1/node_modules/touch/bin/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/Backend/node_modules/.pnpm/touch@3.1.1/node_modules/touch/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/Backend/node_modules/.pnpm/touch@3.1.1/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/Backend/node_modules/.pnpm/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/Backend/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/node_modules:/mnt/c/Users/admin/3D Objects/node_modules:/mnt/c/Users/admin/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/admin/3D Objects/A React 2025/Medium/Backend/node_modules/.pnpm/touch@3.1.1/node_modules/touch/bin/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/Backend/node_modules/.pnpm/touch@3.1.1/node_modules/touch/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/Backend/node_modules/.pnpm/touch@3.1.1/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/Backend/node_modules/.pnpm/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/Backend/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/node_modules:/mnt/c/Users/admin/3D Objects/node_modules:/mnt/c/Users/admin/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/admin/3D Objects/A React 2025/Medium/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
